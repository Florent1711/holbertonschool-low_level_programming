/**
 * _atoi - transforme une chaine de caractères
 * @s : chaine de car à transformer
 *
 * Description: Ascii TO Integer
 *
 * Return: conversion
 */

int _atoi(char *s)
{
        int sign;
	unsigned int num;
	char *temp;

	temp = s;
	num = 0;
	sign = 1;

	while(*temp != '\0' && (*temp < '0' || *temp > '9'))
		
}
